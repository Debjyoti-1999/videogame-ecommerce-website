<%layout('./layout.ejs')%>
<%if(orders){%>
   <% for(let i=0;i<orders.length;i++){%>
   <div class="row">
   <div class="col-sm-6 col-md-4 col-md-offset-3">
    <ul class="list-group">
    <li class='list-group-item'><b>DATE(YY/MM/DD):<%-orders[i].date%></b></li>
    <%let obj=orders[i].cart.items%>
      <%for(key in obj){%>
        <li class="list-group-item">
        <span class="badge"><%-obj[key].qty%></span>
        <strong><%-obj[key].item.title%></strong>
        <span class="label label-success">$<%-obj[key].price%></span>
      </li>
    <%}%>
    <li class='list-group-item'><b><span class='label label-danger' id="total">TOTAL:  $<%-orders[i].cart.totalPrice%></span></b></li>
    </ul>
  </div>
</div>
<%}%>
<%}%>